@model IEnumerable<Salao.Domain.Models.Admin.PreContato>

@{
    ViewBag.Title = "Pré contatos aprovados";
}

<h2>Pré contatos aprovados</h2>

<table class="table top20">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomeSalao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContatoEm)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AtendidoPorUsuario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomeSalao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContatoEm)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AtendidoPorUsuario)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalhes", "Details", new { id=item.Id }) |
        </td>
    </tr>
}

</table>
